{
  "typescript.preferences.quoteStyle": "single",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.suggestionActions.enabled": true,
  "typescript.suggest.enabled": true,
  "typescript.suggest.autoImports": true,
  "coc.source.around.enable": false,
  "coc.source.buffer.enable": false,
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnTypeFiletypes": [ "typescript", "json"],
  "coc.preferences.formatOnSaveFiletypes": [ "javascript"],
  "diagnostic.level": "hint",
  "diagnostic.virtualText": true,
  "git.addGBlameToVirtualText": true,
  "cSpell.enabled": false,
  "explorer.icon.enableVimDevicons": true,
  "explorer.icon.enableNerdfont": true,
  "diagnostic.errorSign": "ÔÅ±",
  "suggest.noselect": false,
  "suggest.enablePreview": true,
  "suggest.echodocSupport": true,
  "suggest.enablePreselect": true,
  "codeLens.enable": true, 
  "prettier.singleQuote": true,
  "prettier.tslintIntegration": true,
  "prettier.semi": true,
  "languageserver": {
    "cquery": {
      "command": "cquery",
      "args": ["--log-file=/tmp/cq.log"],
      "revealOutputChannelOn": "never",
      "filetypes": ["c", "cpp"],
      "initializationOptions": {
        "cacheDirectory": "/tmp/cquery"
      }
    },
    "golang": {
      "command": "bingo",
      "filetypes": ["go"],
      "initializationOptions": {
        "gocodeCompletionEnabled": true,
        "diagnosticsEnabled": true,
        "lintTool": "golint"
      }
    }
  }
}
